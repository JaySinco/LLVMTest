{
    "$schema": "https://raw.githubusercontent.com/microsoft/vcpkg/master/scripts/vcpkg.schema.json",
    "name": "prototyping",
    "version-string": "0.1.0",
    "dependencies": [
        "fmt",
        "gflags",
        "glog",
        "nlohmann-json",
        "boost-algorithm",
        "glfw3",
        {
            "name": "imgui",
            "features": [
                "opengl3-binding",
                "glfw-binding"
            ]
        },
        {
            "name": "opencv4",
            "default-features": true,
            "features": [
                "world"
            ]
        },
        "libtorch-ce",
        "python3-ce",
        "pybind11-ce",
        "mujoco-ce"
    ]
}
