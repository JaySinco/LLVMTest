{
    "$schema": "https://raw.githubusercontent.com/microsoft/vcpkg/master/scripts/vcpkg.schema.json",
    "name": "prototyping",
    "version-string": "0.1.0",
    "dependencies": [
        "fmt",
        "gflags",
        "glog",
        "nlohmann-json",
        "wil",
        {
            "name": "opencv4",
            "default-features": true,
            "features": [
                "world"
            ]
        },
        {
            "name": "llvm",
            "default-features": false,
            "features": [
                "default-options",
                "default-targets"
            ]
        },
        "antlr4"
    ]
}
